@Article{Reyes:2011:ACG,
  author        = {Reyes, R. and de Sande, F.},
  title         = {Automatic code generation for {GPUs} in \texttt{llc}},
  year          = {2011},
  month         = mar,
  pages         = {349--356},
  journal       = {The Journal of Supercomputing},
  volume        = {58},
  number        = {3},
  issn          = {0920-8542 (Print) 1573-0484 (Online)},
  publisher     = {Springer Netherlands},
  impact_f      = {0.687},
  bib2html_pubtype={Journal},
  opturl        = {http://dx.doi.org/10.1007/s11227-011-0591-7},
  optabstract   = {llc is a C-based language where parallelism is expressed
                  using compiler directives. In this paper, we present a new
                  backend of an llc compiler that produces code for GPUs. We
                  have also implemented a software architecture that eases
                  the development of new backends. Our design represents an
                  intermediate layer between a high-level parallel language
                  and different hardware architectures. We evaluate our
                  development by comparing the OpenMP and llc
                  parallelizations of three different algorithms. In every
                  case, the probable performance loss with respect to a
                  direct CUDA implementation is clearly compensated by a
                  significantly smaller development effort.}
}

@InProceedings{Reyes:2011:CSA,
