@InProceedings{Almeida:2003:IBH,
  author        = {Almeida, Francisco and Mediavilla, Evencio and Oscoz, Alex
                  and de Sande, F.},
  editor        = {Joubert, G. R. and Nagel, W. E. and Peters, F. J. and
                  Walter, W. V.},
  title         = {Improvements in black hole detection using parallelism},
  booktitle     = {Parallel computing: Software Technology, Algorithms,
                  Architectures \& Applications},
  series        = {Advances in Parallel Computing},
  year          = {2004},
  pages         = {387--394},
  volume        = {13},
  isbn          = {0-444-51689-1},
  address       = {Dresden, Germany},
  optmonth      = {September},
  optnote       = {Proc. of the Parallel computing (ParCo 2003) 2 -- 5
                  September 2003, Dresden, Germany},
  numdoc        = {8},
  bib2html_pubtype={Refereed Conference},
  optabstract   = {Very frequently there is a divorce between computer
                  scientists and researchers in some other scientific
                  disciplines. The first group have the knowledge and the
                  second one have the problems and the needs of
                  high-performance computing techniques. This work presents
                  the result of a collaboration where we have applied
                  parallelism to improve performance of an astrophysics code
                  used to measure different properties of the accretion disk
                  of a black hole. Several parallelizations have been
                  developed varying from message-passing to shared-memory and
                  hybrid solutions. We present computational results using a
                  CC-NUMA SGI Origin 3000. Este trabajo presenta el resultado
                  de una colaboraci{\'{o}}n en la que se hemos utilizado
                  t{\'{e}}cnicas de computaci{\'{o}}n de altas prestaciones
                  para mejorar la eficiencia de un c{\'{o}}digo
                  astrof{\'{\i}}sico que se utiliza para medir diferentes
                  propiedades del disco de acreci{\'{o}}n de un agujero
                  negro. Se presentan diferentes formas de
                  paralelizaci{\'{o}}n que var{\'{\i}}an desde paso de
                  mensajes hasta memoria compartida as{\'{\i}} como
                  soluciones h{\'{\i}}bridas. Se muestran los resultados
                  obtenidos utilizando una arquitectura CC-NUMA.}
}

@InProceedings{Almeida:2004:PMC,
  author        = {Almeida, Francisco and Delgado, Carlos and L{\'{o}}pez,
                  Ram{\'{o}}n J. Garc{\'{\i}}a and de Sande, F.},
  title         = {Parallelization of a Monte Carlo simulation for a space
                  cosmic particles detector},
  booktitle     = {Proc. of the 11th European PVM/MPI Users' Group Meeting in
                  conjunction with DAPSYS'04 (EuroPVM/MPI 2004)},
  series        = {Lecture Notes in Computer Science},
  year          = {2004},
  month         = {September 19--22},
  pages         = {417--424},
  howpublished  = {\href{http://www.springerlink.com/index/W48P5DKE83D95LAG}{\small{http://www.springerlink.com/index/W48P5DKE83D95LAG}}}
                  ,
  volume        = {3241},
  isbn          = {3-540-23163-3},
  issn          = {0302-9743},
  address       = {Budapest, Hungary},
  note          = {\href{http://www.springerlink.com/index/W48P5DKE83D95LAG}{\small{http://www.springerlink.com/index/W48P5DKE83D95LAG}}}
                  ,
  numdoc        = {12},
  optnumdoc     = {45},
  bib2html_pubtype={LNCS},
  optabstract   = {Work in progress in the design of a parallel simulation of
                  a multipurpose cosmic rays detector is presented. The
                  detector is part of an experiment whose main goal is to
                  study the spectrum and composition of charged cosmic rays
                  with unprecedent sensibility. A sequential version of the
                  simulator based on Monte Carlo method is currently
                  available, but it is ineffective for the study of large
                  samples of moderately high charged particles. Parallelism
                  seems to be the most suitable approach to increase its
                  performance. Driven by different reasons we have decided to
                  take the sequential code as a starting point instead of
                  producing a parallel version from scratch. In this work we
                  present the preliminary computational results that we have
                  obtained for our first prototype of the parallel MPI
                  \texttt{master-slave} simulator on a PC cluster platform.
                  En este trabajo presentamos el estado actual del trabajo
                  que estamos realizando en el dise{\~{n}}o de un simulador
                  paralelo de un detector de rayos c{\'{o}}smicos. El
                  detector es parte de un experimento cuyo principal objetivo
                  es el estudio del espectro y composici{\'{o}}n de rayos
                  c{\'{o}}smicos cargados llegando a unos niveles de
                  sensibilidad nunca alcanzados hasta ahora. Disponemos de
                  una versi{\'{o}}n secuencial del simulador, basada en el
                  m{\'{e}}todo de Monte Carlo. No obstante esta versi{\'{o}}n
                  es ineficiente para el estudio de grandes muestras de
                  part{\'{\i}}culas moderadamente cargadas. La
                  aplicaci{\'{o}}n de t{\'{e}}nicas de computaci{\'{o}}n de
                  altas prestaciones parece la aproximaci{\'{o}}n m{\'{a}}s
                  adecuada para incrementar la eficiencia de esta
                  versi{\'{o}}n secuencial. Debido a diferentes motivos hemos
                  decidido tomar el c{\'{o}}digo secuencial como punto de
                  partida en lugar de dise{\~{n}}ar directamente una
                  versi{\'{o}}n paralela. En este trabajo se presentan los
